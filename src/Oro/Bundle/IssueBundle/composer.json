{
    "name": "oro/issue-bundle",
    "type": "symfony-bundle",
    "description": "Oro Issue Bundle",
    "keywords": ["Oro", "Issue"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "require": {
        "oro/platform": "1.10.*"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\IssueBundle": "src/" }
    },
    "target-dir": "src/Oro/Bundle/IssueBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
